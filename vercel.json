{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/**.py", "use": "@vercel/python" },
    { "src": "backend/**.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/backend/(.*)", "dest": "/backend/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
